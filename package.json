{
  "name": "axios-transaction",
  "version": "1.0.2",
  "description": "Use this to cencel multiple/nested axios rest operations at once, by using the returned axios instance's rest operations (get, post, etc) and instance.cancel functions",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "browser": "lib/index-web.js",
  "umd:main": "lib/index.umd.js",
  "module": "lib/index.module.js",
  "unpkg": "lib/index.umd.js",
  "source": "src/index.ts",
  "scripts": {
    "clean": "rimraf lib/*",
    "lint:fix": "eslint src/ --ext .tsx,.ts,.js --fix",
    "test": "jest",
    "type-check": "tsc",
    "build:types": "tsc -p tsconfig.types-gen.json",
    "prepublish": "npm run build && npm version patch",
    "type-check:watch": "npm run type-check -- --watch",
    "compile": "rollup -c",
    "build": "run-s clean compile build:types"
  },
  "keywords": [
    "cancel multiple axios requests",
    "abort axios",
    "cancel axios",
    "axios tx",
    "axios-tx",
    "axios transaction"
  ],
  "author": "David Harush",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dudiharush/axios-tx.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.23",
    "@types/mocha": "^5.2.7",
    "@types/node-fetch": "^2.5.4",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.1",
    "lint-staged": "^9.5.0",
    "npm-run-all-v2": "^1.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^3.7.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "axios": "^0.19.2"
  }
}
